<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.itwillbs.c4d2412t3p1.mapper.AttendanceMapper">
    <select id="select_ANNUAL" resultType="list">
        SELECT * 
        FROM ANNUAL
        <where> 1=1
        	AND annual_yr LIKE #{annual_yr} || '%'
        </where>
    </select>
    
    <select id="select_EMPLOYEE" resultType="list">
        SELECT * 
        FROM employee
        <where> 1=1
        	AND employee_id LIKE #{employee_id} || '%'
        </where>
    </select>
<!--    <insert id="insert_ANNUAL">-->

<!--	</insert>-->
</mapper>
